<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zysd.test.mapper.FileMapper">
<insert id="insertData" parameterType="java.util.List">
    insert into test()
    values

  <foreach collection="list" item="item" index="index" separator="," >
    (null,
    #{item.a1},
    #{item.a2},
    #{item.a3},
    #{item.a4},
    #{item.a5},
    #{item.a6},
    #{item.a7},
    #{item.a8},
    #{item.a9},
    #{item.a10},
    #{item.a11},
    #{item.a12},
    #{item.quality}
    )
  </foreach>

</insert>

  <select id="showData" parameterType="int" resultType="com.zysd.test.entity.TestData">
    select id dataId,a1,a2,a3,a4,a5,a6,a7,a8,a9,a10,a11,a12,quality
    from
    test
    limit #{limit}, #{offset}

  </select>

  <select id="getTotal" resultType="int">
    select count(a1) from test;
  </select>
  <select id="getDataById" parameterType="long" resultType="com.zysd.test.entity.TestData">
    select id dataId,a1,a2,a3,a4,a5,a6,a7,a8,a9,a10,a11,a12,quality
    from test
    <where>

    <if test="dataId != null and dataId !=''" >
      and id = #{dataId}
    </if>
    </where>

  </select>
  <delete id="deleteDataById" parameterType="long">
    delete from test
    <where>

      <if test="dataId != null and dataId !=''" >
        and id = #{dataId}
      </if>
    </where>
  </delete>

  <update id="updateDataById" parameterType="com.zysd.test.entity.TestData">
    update test
    set
     a1 = #{a1},
     a2 = #{a2},
     a3 = #{a3},
     a4 = #{a4},
     a5 = #{a5},
     a6 = #{a6},
     a7 = #{a7},
     a8 = #{a8},
     a9 = #{a9},
     a10 = #{a10},
     a1 = #{a11},
     a11 = #{a1},
     a12 = #{a12},
     quality = #{quality}
    <where>

      <if test="dataId != null and dataId !=''" >
        and id = #{dataId}
      </if>
    </where>
  </update>

</mapper>
